    "vue": "^3.2.47",
    "vue-echarts": "^6.5.4",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^18.15.11",
    "@types/pako": "^2.0.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "eslint-plugin-vue": "^9.10.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7",
    "typescript": "~5.0.3",
    "vite": "^4.2.3",
